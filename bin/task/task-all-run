base=`cd $(dirname $BASH_SOURCE) && pwd` && source $base/../../bootstrap.sh

log_file="$base/../../var/task-all-run.log"
name_pattern="*.less"
path_pattern="$app_base/test-cases/less/*"
path="$app_base/test-cases"
task_exec="$app_base/test-cases/test.sh"
verbose=1

source $app_vendor_zerustech_cli_bin/task/task-all-run
